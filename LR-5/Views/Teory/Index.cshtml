@model List<Teory>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Теоретические занятия</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>

    <div class="container">
<h4><a asp-action="Create" class="btn btn-primary">Добавить запись</a></h4>
<table class="table">
    <thead>
        <tr>
            <th>Группа</th>
            <th>Тип группы</th>
            <th>Дата</th>
            <th>Время</th>
            <th>Изменить запись</th>
            <th>Удалить запись</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teory in Model)
        {
            <tr>
                <td>@teory.Group.GroupId</td>
                <td>@teory.Group.TypeGroup</td>
                <td>@teory.DateL</td>
                <td>@teory.TimeL</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@teory.Id" class="btn btn-info">Изменить</a>
                </td>
                <td>
                    <form asp-controller="Teory" asp-action="Delete" method="post" asp-route-id="@teory.Id">
                        <button type="submit" class="btn btn-danger" onclick="return confirm('Вы точно хотите удалить запись?')">Удалить</button>
                    </form>
                </td>


            </tr>
        }
    </tbody>
</table>
    </div>


    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

</body>
</html>